{
  "name": "spokestack",
  "version": "3.1.0-beta.4",
  "description": "Tools for integration with the Spokestack API in Node.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run clean && rollup --config && npm run minify:worker",
    "clean": "rm -rf dist/",
    "docs": "typedoc --excludePrivate --plugin typedoc-plugin-markdown --hideBreadcrumbs --out docs --readme none src/index.ts src/client.ts && node tasks/docs.js",
    "format": "eslint . --fix && prettier --write \"**/*.ts\" \"**/*.js\" \"**/*.md\" \"**/*.json\"",
    "lint": "concurrently --raw \"eslint .\" \"npm run prettier\" \"npm run typescript\"",
    "minify:worker": "uglifyjs --compress --mangle --output dist/web-worker.min.js -- dist/web-worker.js",
    "prepare": "husky install && npm run build",
    "prettier": "prettier --check \"**/*.md\" \"**/*.json\"",
    "release": "release-it",
    "start": "rollup --config -w",
    "test": "npm run lint && npm run test:unit",
    "test:next": "cd examples/with-next && npm test",
    "test:unit": "cross-env TS_NODE_PROJECT=tsconfig.test.json mocha",
    "test:watch": "npm run test:unit -- -w",
    "typescript": "concurrently --raw \"tsc -p tsconfig.json --noEmit\" \"tsc -p src/worker/tsconfig.json --noEmit\""
  },
  "files": [
    "dist",
    "src",
    "client.js",
    "client.d.ts",
    "MIT-License.txt",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spokestack/node-spokestack.git"
  },
  "keywords": [
    "spokestack",
    "voice",
    "node"
  ],
  "author": "Timmy Willison <4timmywil@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/spokestack/node-spokestack/issues"
  },
  "homepage": "https://github.com/spokestack/node-spokestack#readme",
  "dependencies": {
    "@google-cloud/speech": "^4.4.0",
    "analyser-frequency-average": "^1.0.0",
    "node-fetch": "^2.6.1",
    "uuid": "^8.3.2",
    "ws": "^7.4.4"
  },
  "devDependencies": {
    "@babel/runtime": "^7.13.10",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@release-it/conventional-changelog": "^2.0.1",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@tensorflow/tfjs": "^3.3.0",
    "@types/express": "^4.17.11",
    "@types/mocha": "^8.2.2",
    "@types/node-fetch": "^2.5.8",
    "@types/sinon": "^9.0.11",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "mocha": "^8.3.2",
    "prettier": "^2.2.1",
    "release-it": "^14.5.0",
    "rollup": "^2.44.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "sinon": "^10.0.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.33",
    "typedoc-plugin-markdown": "^3.6.0",
    "typescript": "^4.2.3",
    "uglify-js": "^3.13.3"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "arrowParens": "always",
    "trailingComma": "none"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "prettier"
    ],
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "project": "./tsconfig.test.json"
    },
    "globals": {
      "ga": "readonly"
    },
    "rules": {
      "eqeqeq": [
        "error",
        "always",
        {
          "null": "ignore"
        }
      ],
      "prettier/prettier": "error",
      "no-unused-vars": "off",
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "@typescript-eslint/no-var-requires": 0,
      "@typescript-eslint/no-use-before-define": 0,
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ]
    }
  },
  "eslintIgnore": [
    "custom_tfjs",
    "dist",
    "examples",
    ".next",
    "*.d.ts",
    "tfjs.js",
    "web-worker.js"
  ],
  "release-it": {
    "hooks": {
      "before:init": "npm run build && npm test",
      "after:bump": "npm run docs && git add README.md"
    },
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true,
      "tokenRef": "SPOKESTACK_GITHUB_TOKEN"
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "conventionalcommits",
          "types": [
            {
              "type": "feat",
              "section": "Features"
            },
            {
              "type": "fix",
              "section": "Bug Fixes"
            },
            {
              "type": "perf",
              "section": "Performance Improvements"
            }
          ]
        }
      }
    }
  }
}
