{
  "name": "spokestack-web-demo",
  "private": true,
  "version": "1.0.0",
  "description": "Spokestack web demo to search and read results using voice.",
  "main": "server/index.js",
  "scripts": {
    "build": "next build && tsc --project tsconfig.server.json",
    "clean": "rm -rf .next/",
    "dev": "nodemon",
    "eslint": "eslint .",
    "format": "prettier --write \"**/*.tsx\" \"**/*.ts\" \"**/*.js\" \"**/*.md\"",
    "lint": "concurrently --raw \"npm run type-check\" \"npm run eslint\" \"npm run prettier\"",
    "lint:fix": "concurrently --raw \"npm run type-check\" \"lint-staged\"",
    "prettier": "prettier --check \"**/*.tsx\" \"**/*.ts\" \"**/*.js\" \"**/*.md\"",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "test": "npm run lint",
    "test:encrypt": "ts-node --project tsconfig.server.json server/encryptSecret.ts",
    "type-check": "tsc -p tsconfig.json --noEmit"
  },
  "keywords": [
    "spokestack",
    "voice"
  ],
  "author": "Timmy Willison <4timmywil@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "apollo-boost": "^0.4.9",
    "babel-plugin-transform-optional-chaining": "^7.0.0-beta.3",
    "body-parser": "^1.19.0",
    "color": "^3.1.3",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.0",
    "graphiql": "^1.0.5",
    "graphql": "^15.3.0",
    "graphql-tag": "^2.11.0",
    "lodash": "^4.17.20",
    "next": "^9.5.1",
    "next-transpile-modules": "^4.0.2",
    "node-fetch": "^2.6.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "spokestack": "^0.3.0",
    "vscode-languageserver-types": "^3.15.1"
  },
  "devDependencies": {
    "@types/color": "^3.0.1",
    "@types/dom-mediacapture-record": "^1.0.7",
    "@types/express": "^4.17.8",
    "@types/express-fileupload": "^1.1.5",
    "@types/lodash": "^4.14.159",
    "@types/node": "^14.0.27",
    "@types/node-fetch": "^2.5.7",
    "@types/react": "^16.9.44",
    "@types/shelljs": "^0.8.8",
    "@types/ws": "^7.2.7",
    "concurrently": "^5.3.0",
    "eslint": "^7.6.0",
    "eslint-plugin-react": "^7.20.5",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.2",
    "shelljs": "^0.8.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "prettier": {
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "jsxBracketSameLine": true,
    "arrowParens": "always",
    "trailingComma": "none"
  }
}
