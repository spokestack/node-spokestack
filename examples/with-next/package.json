{
  "name": "spokestack-web-demo",
  "private": true,
  "version": "1.0.0",
  "description": "Spokestack web demo to search and read results using voice.",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run clean && tsc -p tsconfig.server.json && next build",
    "clean": "rm -rf .next/ dist/",
    "dev": "nodemon",
    "schema": "node apollo/schema.js && graphql-codegen && prettier --write types.ts",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "typescript": "tsc -p tsconfig.json --noEmit"
  },
  "keywords": [
    "spokestack",
    "voice"
  ],
  "author": "Timmy Willison <4timmywil@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "apollo-boost": "^0.4.9",
    "body-parser": "^1.19.0",
    "color": "^4.0.1",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "graphql": "^15.7.1",
    "graphql-tag": "^2.12.5",
    "lodash": "^4.17.21",
    "next": "^12.0.1",
    "node-fetch": "^2.6.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "spokestack": "file:../..",
    "vscode-languageserver-types": "^3.16.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.2.1",
    "@graphql-codegen/typescript": "^2.2.4",
    "@graphql-inspector/cli": "^3.0.2",
    "@types/color": "^3.0.2",
    "@types/dom-mediacapture-record": "^1.0.10",
    "@types/express": "^4.17.13",
    "@types/express-fileupload": "^1.1.7",
    "@types/lodash": "^4.14.176",
    "@types/node": "^16.11.11",
    "@types/node-fetch": "^2.5.12",
    "@types/react": "^17.0.33",
    "@types/shelljs": "^0.8.9",
    "concurrently": "^6.3.0",
    "graphql-playground-middleware-express": "^1.7.22",
    "nodemon": "^2.0.14",
    "shelljs": "^0.8.5",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
